<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="mbti">
  
  <select id="selectMbtiList" parameterType="map" resultType="mbti">
  	select 
  		no,question
	from 
		mbti_question
	where 
		no between #{cPage} and #{endPage}
  </select>
  
	<insert id="insertList" parameterType="map">
		insert  into
			mbti_data
		values(#{no}, #{memberId}, #{result})
		
	</insert>
	
	
	
	<select id="selectMbtiResult" resultType="map" parameterType="string">
		select *
			from mbti_data
		 where 
		 	 member_id = #{id}		
	</select>
	
  
 </mapper>